{
  "name": "maxtripbot",
  "version": "1.0.0",
  "main": "dist/index.js",
  "type": "commonjs",
  "scripts": {
    "start": "pm2 start dist/index.js --name \"MaxTripBot\"",
    "dev": "nodemon --exec ts-node src/index.ts",
    "remove": "ts-node src/remove-commands.ts",
    "deploy": "ts-node src/deploy-commands.ts",
    "update": "git pull origin main && npm install && tsc && pm2 flush MaxTripBot && pm2 restart MaxTripBot",
    "logs": "pm2 logs MaxTripBot",
    "stop": "pm2 stop MaxTripBot"
  },
  "keywords": [],
